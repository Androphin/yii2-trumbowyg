{
    "name": "androphin/yii2-trumbowyg",
    "description": "Trumbowyg extension for Yii 2",
    "keywords": ["yii", "extension", "widget", "trumbowyg", "wysiwyg", "yii2"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/androphin/yii2-trumbowyg/issues?state=open",
        "source": "https://github.com/androphin/yii2-trumbowyg"
    },
    "authors": [
        {
            "name": "androphin",
            "email": "androphin@ymail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "^2.0.0",
        "bower-asset/trumbowyg": "^2.11.0"
    },
    "autoload": {
        "psr-4": {
            "androphin\\yii2\\extensions\\trumbowyg\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
